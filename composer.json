{
  "name": "dlapps/sitemap-bundle",
  "homepage": "https://github.com/dlapps/sitemap-bundle",
  "description": "Provides simple integration for generating sitemaps in Symfony 3.2+ projects",
  "type": "symfony-bundle",
  "keywords": [
    "dreamlabs",
    "sitemap",
    "generation",
    "sitemap.xml"
  ],
  "require": {
    "php": "^7.1",
    "symfony/symfony": "^3.2",
    "jms/serializer-bundle": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5",
    "squizlabs/php_codesniffer": "^2.7"
  },
  "autoload": {
    "psr-4": {
      "DL\\SitemapBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DL\\SitemapBundle\\Tests\\": "tests/"
    }
  },
  "license": "proprietary",
  "authors": [
    {
      "name": "Petre Pătrașc",
      "email": "petre@dreamlabs.ro",
      "homepage": "https://www.dreamlabs.ro",
      "role": "Developer"
    }
  ],
  "scripts": {
    "test": "vendor/bin/phpunit",
    "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src",
    "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src"
  },
  "config": {
    "sort-packages": true
  }
}
